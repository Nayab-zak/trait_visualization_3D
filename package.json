{
  "name": "threejs-trait-visualization",
  "version": "1.0.0",
  "description": "A TypeScript project using Three.js, OrbitControls, and React for UI with strict typing.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "three.js",
    "typescript",
    "orbitcontrols",
    "react",
    "visualization"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "lucide-react": "^0.525.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-icons": "^5.5.0",
    "three": ">=0.160.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/three": "^0.160.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^30.0.4",
    "rollup": "^4.44.1",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "three": ">=0.160.0"
  },
  "scripts": {
    "start": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "build:lib": "rollup -c",
    "type-check": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "jest --passWithNoTests"
  }
}
